@model Infoblog.Models.Meetings.MeetingsViewModels
@{
                ViewBag.Title = "Möten";
}

<h2>Mina skapade möten</h2>

@if (Model.CreatedMeetingPolls != null && Model.CreatedMeetingPolls.Count > 0)
{
<table class="table table-striped table-hover" id="meetingTimeTable">
    <thead>
        <tr>
            <th>Titel</th>
            <th>Beskrivning</th>
            <th>Skapad av</th>
        </tr>
    </thead>
    @foreach (var m in Model.CreatedMeetingPolls)
    {
    <a href="">
</a>
    <tr>
        <td>@Html.ActionLink(m.Title, "ShowMeeting", new { Id = m.Id })</td>
        <td>@Html.ActionLink(m.Content, "ShowMeeting", new { Id = m.Id })</td>
        <td>@Html.ActionLink(m.Author.FirstName + " " + m.Author.LastName, "ShowMeeting", new { Id = m.Id })</td>
    </tr>
    }
</table>
}
else
{
    <p> Inga skapade möten.</p>
}


<h2>Mina mötesinbjudningar</h2>
